<div class="span4 chick">
  <div class="row">
    <div class="span2 name"><%= @chick.get('name') %></div>
    <div class="span2 details">
      <span class="food label label-important"><%= @chick.get('food') %></span>
      <span class="toilet label label-info"><%= @chick.get('toilet') %></span>
      <span class="fun label label-success"><%= @chick.get('fun') %></span>
      <span class="mood label label-inverse"><%= @chick.get('study') %></span>
      <span class="mood badge"><%= @chick.get('mood') %></span>
      <button class="close">&times;</button>
    </div>
  </div>
</div>
